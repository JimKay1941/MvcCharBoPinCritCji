@model IEnumerable<MvcCharBoPinCritCji.Models.CharBopoPinCrit>

@{
    ViewBag.Title = "Subset";
}

<h2>Subset</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Char)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Bopo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Pin)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Crit)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Cji)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Char)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Bopo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Pin)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Crit)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Cji)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new {id = item.ID}) |
                @Html.ActionLink("Details", "Details", new {id = item.ID}) |
                @Html.ActionLink("Delete", "Delete", new {id = item.ID})
            </td>
        </tr>
    }

</table>
